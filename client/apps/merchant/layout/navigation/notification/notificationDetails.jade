template(name="notificationDetails")
  .dropdown-menu(role="menu")
    .header-caption NHẮC NHỞ CHUNG
    ul.details
      each notifies
        li.notification(class="{{hasRead}}")
          //+avatarImageComponent(avatar=notificationSenderAvatar alias=notificationSenderAlias)
          .content
            p.text {{message}}
            p.timestamp {{momentFromNow version.createdAt}}