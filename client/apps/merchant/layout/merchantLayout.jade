template(name="merchantLayout")
  +watermark
  .application(class="{{collapse}} {{homeClass}}")
    +Template.dynamic(template='navigation' data=navigationData)
    +Template.dynamic(template='sidebar' data=sidebarData)
    #container
      if container
        +Template.dynamic(template=container data=containerData)
      else
        +Template.dynamic(template=header data=headerData)
        +Template.dynamic(template=content data=contentData)
        +Template.dynamic(template=footer data=footerData)

  .unsupported-resolution
